[include]
        path = ~/.gitconfig.local
[alias]
	supdate = submodule update --remote
	tags = tag -l
	branches = branch -a
	remotes = remote -v
	graph = log --decorate --all --one-line --graph --color=always --abbrev-commit --topo-order--pretty='%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
[core]
	editor = code --wait
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	grep = auto
